{
  "name": "ui",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "pnpm": "=9.5.0",
    "node": ">=16.14"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "build:export": "NEXT_EXPORT_BUILD=true next build --no-lint",
    "start": "next start",
    "lint": "next lint --ignore-path .gitignore",
    "lint:all": "pnpm lint --dir .",
    "lint:fix": "pnpm lint:all --fix --ext .ts,.tsx,.js,.jsx",
    "commit": "git cz",
    "test": "jest",
    "test:snap": "jest --maxWorkers=50% --silent --watchAll=false --testMatch='!**/*' --testMatch='**/*.snap.tsx'",
    "test:coverage": "jest --coverage --maxWorkers=50% --silent --watchAll=false --reporters=default --coverageReporters=cobertura --coverageReporters=text-summary --coverageReporters=json-summary --coverageReporters=lcov --testMatch='**/*.{spec,test}.{js,jsx,ts,tsx}'",
    "generate": "plop",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "next telemetry disable && husky install",
    "check-filesize": "node .ci/scripts/check-filesize.js",
    "pre-push-check": "pnpm lint && pnpm test && pnpm build"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^3.3.4",
    "@mui/material": "^5.10.15",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^0.27.2",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "jest-junit": "^15.0.0",
    "jest-styled-components": "^7.2.0",
    "next": "13.4.2",
    "next-cloudinary": "^6.0.1",
    "next-secure-headers": "^2.2.0",
    "next-seo": "^5.15.0",
    "notistack": "^2.0.8",
    "query-string": "^7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.0",
    "react-redux": "^8.0.5",
    "react-use": "^17.5.0",
    "yet-another-react-lightbox": "^3.17.0",
    "zod": "^3.22.4"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/jest": "^11.10.5",
    "@nimbleways/eslint-config": "^0.3.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@tsconfig/next": "^1.0.5",
    "@tsconfig/strictest": "^2.0.1",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-redux": "^7.1.25",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "git-cz": "^4.9.0",
    "glob-all": "^3.3.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^14.0.0",
    "next-http-proxy-middleware": "^1.2.5",
    "plop": "^3.1.1",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "typescript": "5.0.4"
  }
}